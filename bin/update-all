#!/bin/bash -e

STAGE=$1

yarn run sls invoke -f updateDaily        --stage ${STAGE} --log --path events/updateDaily/input.${STAGE}.json
yarn run sls invoke -f updateWeekly       --stage ${STAGE} --log --path events/updateWeekly/input.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/android.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/aws.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/docker.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/git.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/go.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/ios.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/java.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/javascript.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/linux.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/nodejs.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/php.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/python.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/rails.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/react.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/ruby.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/swift.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/typescript.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/vim.${STAGE}.json
yarn run sls invoke -f updateWeeklyByTag  --stage ${STAGE} --log --path events/updateWeeklyByTag/vuejs.${STAGE}.json
