yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/react.${STAGE}.json
yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/ruby.${STAGE}.json
yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/swift.${STAGE}.json
yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/typescript.${STAGE}.json
yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/vim.${STAGE}.json
yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/vuejs.${STAGE}.json
yarn run sls invoke -f updateDailyByTag --stage ${STAGE} --log --path events/updateDailyByTag/beginner.${STAGE}.json

