#!/bin/bash -e

STAGE=$1

yarn run sls invoke -f updateWeekly --log --path events/updateWeekly/input.${STAGE}.json

yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/aws.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/aws.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/docker.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/go.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/ios.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/java.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/javascript.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/php.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/python.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/rails.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/react.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/ruby.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/swift.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/typescript.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/vim.${STAGE}.json
yarn run sls invoke -f updateWeeklyPerTag --stage ${STAGE} --log --path events/updateWeeklyPerTag/vuejs.${STAGE}.json
